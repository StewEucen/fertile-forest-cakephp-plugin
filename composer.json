{
    "name": "stew-eucen/fertile-forest",
    "description": "The fertile forest model for hierarchical data in a database on CakePHP plugin",
    "keywords": ["FertileForest", "CakePHP", "StewEucen"],
    "type": "cakephp-plugin",
    "license": "MIT",
    "homepage": "http://lab.kochlein.com/FertileForest",
    "authors": [
      {
        "name": "StewEucen",
        "email": "stew.eucen@gmail.com"
      }
    ],
    "require": {
        "php": ">=5.6.3",
        "cakephp/cakephp": ">=3.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "FertileForest\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FertileForest\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
